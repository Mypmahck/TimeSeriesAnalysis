<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!-- 算法参数 -->
<algorithmsParams>
    <!-- 周期检测相关算法参数 -->
    <pmParams>
        <!-- ERP距离与平均熵周期检测 -->
        <ERPandEntropyParams>
            <longestPeriod>300</longestPeriod>
            <threshold>0.2</threshold>
        </ERPandEntropyParams>
    </pmParams>

    <!-- 异常检测算法参数 -->
    <outlierMinerParam>
        <!-- 傅立叶变换 -->
        <fastFourierParams>
            <sizeK>8</sizeK>
            <amplitudeRatio>0.9</amplitudeRatio>
            <varK>3.0</varK>
        </fastFourierParams>
        <!-- 高斯滑动窗口 -->
        <gaussianParams>
            <initWindowSize>30</initWindowSize>
            <maxWindowSize>60</maxWindowSize>
            <expWindowSize>3</expWindowSize>
            <windowVarK>3.0</windowVarK>
        </gaussianParams>
        <!-- 线段模式异常检测 -->
        <pointPatternParams>
            <densityK>2</densityK>
            <patternThreshold>0.2</patternThreshold>
            <neighborK>10</neighborK>
        </pointPatternParams>
        <TEOParams>
            <mergeThreshold>0.025</mergeThreshold>
            <neighborK>7</neighborK>
        </TEOParams>
        <!-- 多维高斯 -->
        <multidimensionalParams>
            <GuassK>4</GuassK>
            <piontK>5</piontK>
            <densityK>2</densityK>
            <patternThreshold>0.1</patternThreshold>
            <mergerPrice>0.05</mergerPrice>
        </multidimensionalParams>
        <saxPatternParams>
            <patternLen>24</patternLen>
            <patternAlphaNum>5</patternAlphaNum>
        </saxPatternParams>
    </outlierMinerParam>

    <!-- 统计参数 -->
    <ssParams>
        <seriesStatisticsParam>
            <splitM>2</splitM>
            <ratio>0.2</ratio>
        </seriesStatisticsParam>
    </ssParams>

    <!-- 频繁项参数 -->
    <sParams>
        <smParam>
            <minSupport>5</minSupport>
            <sizeWindow>10</sizeWindow>
            <stepWindow>1</stepWindow>
            <splitLeastLen>4</splitLeastLen>
            <clusterNum>8</clusterNum>
        </smParam>
    </sParams>

    <!-- 预测相关算法参数 -->
    <forecastParams>
        <!-- 神经网络 -->
        <neuralNetworkParams>
            <predictPeriod>20</predictPeriod>
            <momentum>0.1</momentum>
            <learnRate>0.2</learnRate>
            <seed>0</seed>
            <trianTime>500</trianTime>
        </neuralNetworkParams>
        <!-- ARIMA -->
        <ARIMAParams>
            <predictPeriod>20</predictPeriod>
        </ARIMAParams>
    </forecastParams>

    <!-- 关联规则相关算法参数 -->
    <arParams>
        <!-- 线段 -->
        <associationRuleLineParams>
            <!-- 线段数合并代价参数 -->
            <mergerPrice>0.06</mergerPrice>
            <compressionRatio>0.65</compressionRatio>
            <!-- 线段聚类参数 -->
            <t>0.1</t>
            <centerLine>-1.0</centerLine>
            <way>1</way>
            <gaosi>3</gaosi>
            <multiOriMin>false</multiOriMin>
        </associationRuleLineParams>
        <!-- 相似度 -->
        <associationRuleSimilarityParams>
            <supportThresh>0.4,10</supportThresh>
            <bias>0</bias>
            <whichAlgorithm>1</whichAlgorithm>
        </associationRuleSimilarityParams>
    </arParams>

    <!-- pcap解析参数 -->
    <pcapParseParams>
        <pcapPath></pcapPath>
        <outputPath></outputPath>
    </pcapParseParams>
</algorithmsParams>