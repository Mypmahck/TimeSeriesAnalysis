<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!-- 算法参数 -->
<algorithmsParams>
    <!-- 周期检测相关算法参数 -->
    <pmParams>
        <!-- ERP距离与平均熵周期检测 -->
        <ERPandEntropyParams>
            <longestPeriod>300</longestPeriod>
            <threshold>0.2</threshold>
        </ERPandEntropyParams>
    </pmParams>

    <!-- 异常检测算法参数 -->
    <outlierMinerParam>
        <!-- 傅立叶变换 -->
        <fastFourierParams>
            <sizeK>8</sizeK>
            <amplitudeRatio>0.8</amplitudeRatio>
            <varK>3.0</varK>
	    <diff>0.2</diff>
        </fastFourierParams>
        <!-- 高斯滑动窗口 -->
        <gaussianParams>
            <initWindowSize>200</initWindowSize>
            <maxWindowSize>300</maxWindowSize>
            <expWindowSize>3</expWindowSize>
            <windowVarK>3.0</windowVarK>
	    <diff>0.2</diff>
            <minthreshod>0.7</minthreshod>
        </gaussianParams>
        <!-- 线段模式异常检测 -->
        <pointPatternParams>
            <densityK>3</densityK>
            <patternThreshold>0.1</patternThreshold>
            <neighborK>20</neighborK>
	    <diff>0.1</diff>
	    <threshold>0.7</threshold>
        </pointPatternParams>
        <TEOParams>
            <mergeThreshold>0.025</mergeThreshold>
            <neighborK>7</neighborK>
        </TEOParams>
        <!-- 多维高斯 -->
        <multidimensionalParams>
            <GuassK>4</GuassK>
            <piontK>3</piontK>
            <densityK>4</densityK>
            <patternThreshold>0.1</patternThreshold>
            <mergerPrice>0.05</mergerPrice>
        </multidimensionalParams>
        <saxPatternParams>
            <patternLen>24</patternLen>
            <patternAlphaNum>5</patternAlphaNum>
        </saxPatternParams>
	<!-- 基于周期 -->
	<periodBasedParams>
	     <GuassK>3.0</GuassK>
	     <threshold>0.8</threshold>
	</periodBasedParams>
    </outlierMinerParam>

    <!-- 统计参数 -->
    <ssParams>
        <seriesStatisticsParam>
            <splitM>2</splitM>
            <ratio>0.2</ratio>
        </seriesStatisticsParam>
    </ssParams>

    <!-- 频繁项参数 -->
    <sParams>
        <smParam>
            <minSupport>3</minSupport>
            <sizeWindow>10</sizeWindow>
            <stepWindow>1</stepWindow>
            <splitLeastLen>16</splitLeastLen>
            <clusterNum>4</clusterNum>
        </smParam>
    </sParams>

    <!-- 预测相关算法参数 -->
    <forecastParams>
        <!-- 神经网络 -->
        <neuralNetworkParams>
            <predictPeriod>10</predictPeriod>
            <momentum>0.1</momentum>
            <learnRate>0.2</learnRate>
            <seed>0</seed>
            <trianTime>500</trianTime>
        </neuralNetworkParams>
        <!-- ARIMA -->
        <ARIMAParams>
            <predictPeriod>10</predictPeriod>
        </ARIMAParams>
    </forecastParams>

    <!-- 关联规则相关算法参数 -->
    <arParams>
        <!-- 线段 -->
        <associationRuleLineParams>
            <!-- 线段数合并代价参数 -->
            <mergerPrice>0.085</mergerPrice>
            <compressionRatio>0.65</compressionRatio>
            <!-- 线段聚类参数 -->
            <t>0.07</t>
            <centerLine>-1.0</centerLine>
            <way>2</way>
            <gaosi>2</gaosi>
	        <alpha>0.2</alpha>
	        <maxCluster>5</maxCluster>
            <multiOrMin>true</multiOrMin>
        </associationRuleLineParams>
        <!-- 相似度 -->
        <associationRuleSimilarityParams>
            <supportThresh>0.4,10</supportThresh>
            <bias>0</bias>
            <whichAlgorithm>1</whichAlgorithm>
        </associationRuleSimilarityParams>
    </arParams>

    <!-- pcap解析参数 -->
    <pcapParseParams>
        <pcapPath>D:\pcapn</pcapPath>
        <outputPath>D:\out</outputPath>
        <parseAll>true</parseAll>
    </pcapParseParams>

    <!-- 分布式服务端参数 -->
    <serverParams>
        <port>7777</port>
    </serverParams>

    <!-- 分布式pcap解析参数 -->
    <pcapParseDisParams>
        <pcapPath>D:\pcapn</pcapPath>
        <outputPath>D:\out</outputPath>
    </pcapParseDisParams>

    <!-- 客户端参数 -->
    <clientParams>
        <ip>127.0.0.1</ip>
        <port>7777</port>
        <pcapPath>E:\pcapn</pcapPath>
        <outputPath>E:\57data</outputPath>
    </clientParams>
    <!-- 端口 -->
    <portParams>
        <port>80</port>
        <port>9</port>
        <port>20</port>
        <port>22</port>
        <port>25</port>
        <port>10</port>
        <port>11</port>
        <port>403</port>
        <port>404</port>
        <port>405</port>
        <port>406</port>
        <port>407</port>
        <port>408</port>
        <port>409</port>
        <port>410</port>
    </portParams>
</algorithmsParams>